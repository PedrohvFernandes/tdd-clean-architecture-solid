{
  "name": "tdd-clean-architecture-solid",
  "private": true,
  "version": "1.6.2",
  "scripts": {
    "dev": "vite",
    "dev:open": "npm run dev -- --open",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "preview": "vite preview",
    "test": "jest --passWithNoTests",
    "test:clear": "jest --clearCache",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "test:cypress": "cypress open",
    "test:cypress:run": "cypress run",
    "test:cypress:ci": "start-server-and-test dev http://localhost:3000 test:cypress:run",
    "prepare": "husky",
    "check": "npm-check -u -s"
  },
  "dependencies": {
    "@types/history": "^5.0.0",
    "axios": "^1.6.8",
    "cypress-vite": "^1.5.0",
    "eslint-plugin-cypress": "^3.3.0",
    "history": "^5.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0",
    "tailwind-merge": "^2.2.2",
    "tdd-clean-architecture-solid": "file:",
    "vite-plugin-favicons-inject": "^2.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@pedrohvfernandes/eslint-config": "^0.1.2",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^15.0.6",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.8",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "coveralls": "^3.1.1",
    "cypress": "^13.14.1",
    "eslint": "^8.57.0",
    "git-commit-msg-linter": "^5.0.7",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-localstorage-mock": "^2.4.26",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "start-server-and-test": "^2.0.5",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "ts-jest-mock-import-meta": "^1.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.11"
  }
}
